                <TableColumn fx:id="table_value" editable="false" prefWidth="139.19998779296876" text="Value" />
              </columns>
            </TableView>
            <TextField fx:id="txt_key" layoutX="14.0" layoutY="92.0" prefHeight="26.0" prefWidth="223.0" promptText="Enter The key" />
            <TextField fx:id="txt_value" layoutX="14.0" layoutY="146.0" prefHeight="26.0" prefWidth="223.0" promptText="Enter The Value">
